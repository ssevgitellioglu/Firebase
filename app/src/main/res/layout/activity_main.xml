<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SADECE KAYITLI KULLANICILAR GÖREBİLİR !"
            android:textSize="30sp" android:textAlignment="center"
            android:id="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvKullaniciİd" android:layout_marginBottom="232dp"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/tvKullaniciadi" android:textSize="12sp"
            android:layout_marginTop="27dp" android:layout_marginBottom="23dp"
            app:layout_constraintEnd_toEndOf="@+id/tvKullaniciİd"
            app:layout_constraintBottom_toTopOf="@+id/tvKullanicimail" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tvKullanicimail"/>
    <TextView
            android:layout_width="283dp"
            android:layout_height="0dp"
            android:id="@+id/tvKullanicimail" android:textSize="12sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="34dp" app:layout_constraintTop_toBottomOf="@+id/tvKullaniciadi"
            android:layout_marginBottom="19dp" app:layout_constraintBottom_toTopOf="@+id/tvKullaniciİd"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/tvKullaniciİd" android:textSize="12sp"
            app:layout_constraintTop_toBottomOf="@+id/tvKullanicimail" android:layout_marginBottom="152dp"
            app:layout_constraintEnd_toEndOf="@+id/tvKullanicimail" app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/tvKullanicimail" app:layout_constraintHorizontal_bias="1.0"/>


</androidx.constraintlayout.widget.ConstraintLayout>